/**
 * Session Security Tests
 * Critical authentication and session management security tests
 */

import { describe, it, expect, beforeEach, vi, afterEach } from 'vitest'
import { NextRequest, NextResponse } from 'next/server'
import { getToken } from 'next-auth/jwt'

// Mock next-auth/jwt
vi.mock('next-auth/jwt')
const mockGetToken = getToken as vi.MockedFunction<typeof getToken>

// Mock middleware
import { middleware } from '@/middleware'

describe('Session Security', () => {
  beforeEach(() => {
    vi.clearAllMocks()
    
    // Mock environment variables
    process.env.NEXTAUTH_SECRET = 'test-secret-minimum-32-characters-long-for-security'
    process.env.NODE_ENV = 'test'
  })

  afterEach(() => {
    vi.restoreAllMocks()
  })

  describe('CSRF Protection', () => {
    it('should include CSRF protection headers in responses', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER'
      })

      const response = await middleware(request)
      
      expect(response).toBeInstanceOf(NextResponse)
      expect(response.headers.get('X-Frame-Options')).toBe('DENY')
      expect(response.headers.get('X-Content-Type-Options')).toBe('nosniff')
      expect(response.headers.get('Referrer-Policy')).toBe('strict-origin-when-cross-origin')
    })

    it('should prevent clickjacking attacks with X-Frame-Options', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER'
      })

      const response = await middleware(request)
      
      expect(response.headers.get('X-Frame-Options')).toBe('DENY')
    })

    it('should prevent MIME type sniffing attacks', async () => {
      const request = new NextRequest('http://localhost:3000/api/dashboard/stats')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER'
      })

      const response = await middleware(request)
      
      expect(response.headers.get('X-Content-Type-Options')).toBe('nosniff')
    })

    it('should implement strict referrer policy', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER'
      })

      const response = await middleware(request)
      
      expect(response.headers.get('Referrer-Policy')).toBe('strict-origin-when-cross-origin')
    })

    it('should restrict permissions policy for sensitive features', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER'
      })

      const response = await middleware(request)
      
      expect(response.headers.get('Permissions-Policy')).toBe('camera=(), microphone=(), geolocation=()')
    })
  })

  describe('Session Validation', () => {
    it('should validate JWT token signature', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER',
        exp: Math.floor(Date.now() / 1000) + 3600 // 1 hour from now
      })

      const response = await middleware(request)
      
      expect(mockGetToken).toHaveBeenCalledWith({
        req: request,
        secret: expect.any(String)
      })
      expect(response.status).not.toBe(401)
    })

    it('should reject expired tokens', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER',
        exp: Math.floor(Date.now() / 1000) - 3600 // 1 hour ago (expired)
      })

      const response = await middleware(request)
      
      // Should redirect to login for expired token
      expect(response.status).toBe(307) // Temporary redirect
      expect(response.headers.get('location')).toContain('/login')
    })

    it('should handle malformed tokens gracefully', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockRejectedValue(new Error('Invalid token'))

      const response = await middleware(request)
      
      // Should redirect to login on token error
      expect(response.status).toBe(307)
      expect(response.headers.get('location')).toContain('/login')
    })

    it('should validate token roles for admin routes', async () => {
      const request = new NextRequest('http://localhost:3000/admin/users')
      
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER' // Non-admin role
      })

      const response = await middleware(request)
      
      // Should redirect non-admin users away from admin routes
      expect(response.status).toBe(307)
      expect(response.headers.get('location')).toContain('/dashboard')
    })

    it('should allow admin access to admin routes', async () => {
      const request = new NextRequest('http://localhost:3000/admin/users')
      
      mockGetToken.mockResolvedValue({
        sub: 'admin-123',
        email: 'admin@example.com',
        role: 'ADMIN'
      })

      const response = await middleware(request)
      
      // Should allow admin access
      expect(response.status).not.toBe(307)
    })

    it('should enforce super admin restrictions', async () => {
      const request = new NextRequest('http://localhost:3000/super-admin/system')
      
      mockGetToken.mockResolvedValue({
        sub: 'admin-123',
        email: 'admin@example.com',
        role: 'ADMIN' // Regular admin, not super admin
      })

      const response = await middleware(request)
      
      // Should redirect regular admin away from super admin routes
      expect(response.status).toBe(307)
      expect(response.headers.get('location')).toContain('/dashboard')
    })

    it('should allow super admin access to all routes', async () => {
      const request = new NextRequest('http://localhost:3000/super-admin/system')
      
      mockGetToken.mockResolvedValue({
        sub: 'superadmin-123',
        email: 'superadmin@example.com',
        role: 'SUPER_ADMIN'
      })

      const response = await middleware(request)
      
      // Should allow super admin access
      expect(response.status).not.toBe(307)
    })
  })

  describe('Route Protection', () => {
    it('should allow access to public routes without authentication', async () => {
      const publicRoutes = [
        '/',
        '/pricing',
        '/demo',
        '/login',
        '/register',
        '/consciousness',
        '/vs-competitors'
      ]

      for (const route of publicRoutes) {
        const request = new NextRequest(`http://localhost:3000${route}`)
        mockGetToken.mockResolvedValue(null)

        const response = await middleware(request)
        
        // Should not redirect public routes
        expect(response.status).not.toBe(307)
      }
    })

    it('should protect private routes and redirect to login', async () => {
      const privateRoutes = [
        '/dashboard',
        '/settings',
        '/profile',
        '/admin'
      ]

      for (const route of privateRoutes) {
        const request = new NextRequest(`http://localhost:3000${route}`)
        mockGetToken.mockResolvedValue(null)

        const response = await middleware(request)
        
        // Should redirect private routes to login
        expect(response.status).toBe(307)
        expect(response.headers.get('location')).toContain('/login')
      }
    })

    it('should redirect authenticated users away from auth pages', async () => {
      const authRoutes = ['/login', '/register']

      for (const route of authRoutes) {
        const request = new NextRequest(`http://localhost:3000${route}`)
        mockGetToken.mockResolvedValue({
          sub: 'user-123',
          email: 'test@example.com',
          role: 'USER'
        })

        const response = await middleware(request)
        
        // Should redirect authenticated users to dashboard
        expect(response.status).toBe(307)
        expect(response.headers.get('location')).toContain('/dashboard')
      }
    })

    it('should preserve callback URL for post-login redirect', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard/analytics')
      mockGetToken.mockResolvedValue(null)

      const response = await middleware(request)
      
      expect(response.status).toBe(307)
      const location = response.headers.get('location')
      expect(location).toContain('/login')
      expect(location).toContain('callbackUrl=%2Fdashboard%2Fanalytics')
    })
  })

  describe('Security Headers', () => {
    it('should apply security headers to all responses', async () => {
      const request = new NextRequest('http://localhost:3000/')
      mockGetToken.mockResolvedValue(null)

      const response = await middleware(request)
      
      expect(response.headers.get('X-Frame-Options')).toBe('DENY')
      expect(response.headers.get('X-Content-Type-Options')).toBe('nosniff')
      expect(response.headers.get('Referrer-Policy')).toBe('strict-origin-when-cross-origin')
      expect(response.headers.get('Permissions-Policy')).toBe('camera=(), microphone=(), geolocation=()')
    })

    it('should apply security headers to authenticated responses', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      mockGetToken.mockResolvedValue({
        sub: 'user-123',
        email: 'test@example.com',
        role: 'USER'
      })

      const response = await middleware(request)
      
      expect(response.headers.get('X-Frame-Options')).toBe('DENY')
      expect(response.headers.get('X-Content-Type-Options')).toBe('nosniff')
      expect(response.headers.get('Referrer-Policy')).toBe('strict-origin-when-cross-origin')
      expect(response.headers.get('Permissions-Policy')).toBe('camera=(), microphone=(), geolocation=()')
    })
  })

  describe('Build Phase Handling', () => {
    it('should skip middleware during build phase', async () => {
      // Mock build environment
      process.env.NEXT_PHASE = 'phase-production-build'
      
      const request = new NextRequest('http://localhost:3000/dashboard')

      const response = await middleware(request)
      
      // Should bypass all security checks during build
      expect(mockGetToken).not.toHaveBeenCalled()
      expect(response.status).not.toBe(307)
      
      // Cleanup
      delete process.env.NEXT_PHASE
    })

    it('should skip middleware when building for Vercel', async () => {
      // Mock Vercel build environment
      process.env.BUILDING_FOR_VERCEL = '1'
      
      const request = new NextRequest('http://localhost:3000/dashboard')

      const response = await middleware(request)
      
      // Should bypass all security checks during build
      expect(mockGetToken).not.toHaveBeenCalled()
      expect(response.status).not.toBe(307)
      
      // Cleanup
      delete process.env.BUILDING_FOR_VERCEL
    })

    it('should skip middleware in CI environment', async () => {
      // Mock CI environment
      process.env.CI = 'true'
      
      const request = new NextRequest('http://localhost:3000/dashboard')

      const response = await middleware(request)
      
      // Should bypass all security checks during CI
      expect(mockGetToken).not.toHaveBeenCalled()
      expect(response.status).not.toBe(307)
      
      // Cleanup
      delete process.env.CI
    })
  })

  describe('Error Handling', () => {
    it('should handle auth service errors gracefully', async () => {
      const request = new NextRequest('http://localhost:3000/dashboard')
      
      mockGetToken.mockRejectedValue(new Error('Auth service unavailable'))

      const response = await middleware(request)
      
      // Should redirect to login on auth errors
      expect(response.status).toBe(307)
      expect(response.headers.get('location')).toContain('/login')
    })

    it('should continue without token on auth errors', async () => {
      const request = new NextRequest('http://localhost:3000/')
      
      mockGetToken.mockRejectedValue(new Error('Token validation failed'))

      const response = await middleware(request)
      
      // Should not crash and should apply security headers
      expect(response.headers.get('X-Frame-Options')).toBe('DENY')
    })
  })
})

