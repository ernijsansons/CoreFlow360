    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:subscription": "vitest src/__tests__/subscription",
    "test:ai": "vitest src/__tests__/ai",
    "test:ui:components": "vitest src/__tests__/ui",
    "test:all": "npm run test:subscription && npm run test:ai && npm run test:ui:components",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen localhost:3000",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "postinstall": "npx prisma generate",
    "db:deploy": "npx prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:modules": "tsx prisma/seed-modules.ts",
    "ai:train": "tsx scripts/ai/train-models.ts",
    "ai:deploy": "tsx scripts/ai/deploy-agents.ts",
    "ai:monitor": "tsx scripts/ai/monitor-performance.ts"
  },
